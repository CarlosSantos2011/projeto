
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="aling-itens:center">
    <table class="table table-hover table-condensed">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Sigla</th>
                <th>Ações</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var produto in ViewBag.Produtos)
            {
                <tr >
                    <td>@produto.Id</td>
                    <td>@produto.Nome</td>
                   
                    
                </tr>
            }
    </table>
</div>